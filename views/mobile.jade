extends layout
block head
	script(src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js", type="text/javascript")
	script(src="/js/index.js", type="text/javascript")
	script(src="http://www.mapquestapi.com/sdk/js/v7.0.s/mqa.toolkit.js?key=Fmjtd%7Cluua20utnh%2Cbs%3Do5-967au6")
block content
	.row
		.span12
			h1 Where are we going?
			h3 A look at who is excited about which Startup Weekend
	.row
		.span3
			form.form-search
				input.input-medium.search-query(type="text")
				button.btn(type="submit")
					i.icon-search
			
			ul.nav.nav-list(data-bind="template: {name: 'event-list-template', foreach: events, as: 'event'}")
				
					
		.span9
			.well
				div#map.map
	script(type="text/html", id="event-list-template")
		li
			span(data-bind="text: city") City
	script(type="text/html", id="event-template")
		div Id number
			span(data-bind="text: id")
	script(src="/js/knockout.js", type="text/javascript")
	script(src="/js/knockout.mapping.js", type="text/javascript")
	script(src="/js/mobile.js", type="text/javascript")